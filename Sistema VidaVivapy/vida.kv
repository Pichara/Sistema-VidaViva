#:import Factory kivy.factory.Factory

<sp@SpinnerOption>:
    color: 1,1,1,1
    background_color: 0,0,0,1

<MyInput@Spinner>:
    option_cls: 'sp'
    font_size: 24
    size_hint: None, None
    size: self.texture_size[0] + dp(56), dp(56)
    background_normal: ''
    background_color: 1, 1, 1, 1
    border: (4, 4, 4, 4)
    padding: (10, 10)
    values: [str(i) for i in range(1, 100)]
    text: '1'
    readonly: True
    on_text: self.text = str(int(self.text))  # converte o valor para um inteiro

